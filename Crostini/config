  User chronos
  HostName 100.115.92.1
  Port 22
  IdentityFile ~/.ssh/id_rsa
  PreferredAuthentications publickey
  ControlMaster auto
  ControlPath /tmp/%r@%h:%p
  ControlPersist 1h

